#My Calculator
first = input("Enter First Number : ")
second = input("Enter Second Number : ")
first = int(first)
second = int(second)
print("------Press Keys For Operator (+,-,*,%)-------")
operator = input("Enter operator : ")
if operator == "+" :
    print(first + second)
elif operator == "-" :
    print(first - second)
elif operator == "*" :    
    print(first * second)
elif operator == "/" :   
    print(first / second)
elif operator == "%" :    
    print((first % second))
else : 
    print("Invalid Operator")    
